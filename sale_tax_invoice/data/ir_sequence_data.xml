<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_account_tax_invoice" model="ir.sequence">
            <field name="name">Tax Invoice Sequence</field>
            <field name="code">account.tax.invoice.sequence</field>
            <field name="prefix">TINV/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
            <field name="implementation">standard</field>
        </record>
        <record id="seq_account_invoice" model="ir.sequence">
            <field name="name">Invoice Sequence</field>
            <field name="code">account.invoice.sequence</field>
            <field name="prefix">INV/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
            <field name="implementation">standard</field>
        </record>
    </data>
</odoo>