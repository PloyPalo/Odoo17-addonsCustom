<odoo>
    <!-- Action to open the Commission list view -->
    <record id="action_commissions" model="ir.actions.act_window">
        <field name="name">Commissions</field>
        <field name="res_model">custom.commission</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- Menu item to access the Commission action -->
    <menuitem id="menu_commission_root" name="Commission"
              sequence="10"/>
    <menuitem id="menu_commissions" name="Commissions"
              parent="menu_commission_root" action="action_commissions"
              sequence="20"/>

    <!-- Views for the Commission model -->
    <record id="view_commissions_tree" model="ir.ui.view">
        <field name="name">custom.commission.tree</field>
        <field name="model">custom.commission</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="amount"/>
                <field name="date"/>
            </tree>
        </field>
    </record>

    <record id="view_commissions_form" model="ir.ui.view">
        <field name="name">custom.commission.form</field>
        <field name="model">custom.commission</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="amount"/>
                        <field name="date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>